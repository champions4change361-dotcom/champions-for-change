  /nfl/{season}/player-points:
    get:
      summary: NFL Player Points
      description: Returns fantasy points for NFL players.
      parameters:
        - $ref: '#/components/parameters/Season'
        - name: min
          description: >-
            Controls the level of player information returned in the response. A
            value of `true` will return less player information for a smaller
            response.
          in: query
          required: false
          schema:
            default: 'false'
            type: string
            enum:
              - 'true'
              - 'false'
            example: true
        - name: start
          description: The starting week to return data from.
          in: query
          required: false
          schema:
            type: integer
            default: 1
            example: 5
        - name: end
          description: >-
            The ending week to return data through. Defaults to the final week
            of the regular season for the requested season.
          in: query
          required: false
          schema:
            type: integer
            example: 18
        - name: position
          description: Only return players with this position.
          in: query
          required: false
          schema:
            type: string
            enum:
              - ALL
              - QB
              - RB
              - WR
              - TE
              - OT
              - OL
              - OG
              - C
              - DE
              - DT
              - LB
              - CB
              - S
              - DB
              - K
              - P
              - DST
            default: ALL
            example: QB
        - name: scoring
          description: The scoring type to use to calculate points.
          in: query
          required: false
          schema:
            type: string
            enum:
              - STD
              - PPR
              - HALF
            default: STD
            example: PPR
      responses:
        '200':
          description: A successful response
          content:
            application/json:
              schema:
                type: object
                required:
                  - season
                  - scoring
                  - players
                properties:
                  season:
                    description: The season that was evaluated.
                    type: string
                    example: '2024'
                  scoring:
                    description: The scoring that was evaluated.
                    type: string
                    example: STD
                  players:
                    description: An array containing player objects.
                    type: array
                    items:
                      type: object
                      required:
                        - player_id
                        - games
                        - points
                        - average
                        - weeks
                      properties:
                        player_id:
                          $ref: '#/components/schemas/PlayerId'
                        player_name:
                          $ref: '#/components/schemas/PlayerName'
                        position_id:
                          $ref: '#/components/schemas/PositionId'
                        team_id:
                          $ref: '#/components/schemas/TeamId'
                        filename:
                          $ref: '#/components/schemas/Filename'
                        games:
                          description: The number of games the player played in.
                          type: integer
                          example: 10
                        points:
                          description: The total points the player accrued.
                          type: number
                          example: 142.5
                        average:
                          description: The average number of points the player accrued.
                          type: number
                          example: 14.25
                        weeks:
                          description: An object keyed by week number.
                          type: object
                          patternProperties:
                            ^([1-9]|1[0-8])$:
                              type: number
                              format: float
                          example:
                            '1': 6.9
                            '2': 18.3
                            '3': 19.4
                            '4': 10.5
                            '5': 22.1
                            '6': 12.8
                            '7': 6
                            '8': 7.7
                            '9': 24.4
                            '10': 14.2
      security:
        - api_key: []
  /mlb/lineups:
    get:
      summary: MLB Lineups
      description: Returns MLB lineups.
      parameters:
        - name: start
          description: Date to return lineups for
          in: query
          schema:
            type: string
            example: '2025-05-13'
        - name: period
          description: A season type to return games for
          in: query
          schema:
            type: string
            enum:
              - PRE
              - REG
              - PST
            example: REG
            default: REG
        - name: projected
          description: Return projected lineups for the provided date
          in: query
          schema:
            type: string
            example: 'true'
            default: 'false'
      responses:
        '200':
          description: A successful response
          content:
            application/json:
              schema:
                type: object
                required:
                  - league_key
                  - season
                  - start
                  - end
                  - count
                  - games
                properties:
                  league_key:
                    type: string
                    example: ''
                  season:
                    description: The season queried for games
                    type: string
                    example: '2025'
                  start:
                    description: The date of games
                    type: string
                    example: '2025-06-23'
                  end:
                    description: The date of games
                    type: string
                    example: '2025-06-23'
                  count:
                    type: integer
                    example: 9
                  games:
                    description: An array of games
                    type: array
                    items:
                      type: object
                      required:
                        - game_id
                        - event_id
                        - status
                        - weather
                        - chance_rain
                        - weather_icon
                        - temp
                        - wind
                        - wind_direction
                        - deg_offset
                        - teams
                        - away_probable_id
                        - home_probable_id
                        - hitters
                        - pitchers
                      properties:
                        game_id:
                          type: string
                          example: '97000'
                        event_id:
                          type: string
                          example: '97000'
                        status:
                          type: string
                          example: scheduled
                        weather:
                          type: string
                          example: Mostly Clear, 97° F, 8 mph wind
                        chance_rain:
                          type: number
                          example: 0
                        weather_icon:
                          type: string
                          format: url
                          example: >-
                            https://images.fantasypros.com/images/icons/weather/cloud.png
                        temp:
                          description: Gametime temperature in fahrenheit
                          type: integer
                          example: 97
                        wind:
                          description: Gametime windspeed in miles per hour
                          type: integer
                          example: 8
                        wind_direction:
                          type: Wind direction in degrees clockwise from north
                          example: 308
                        deg_offset:
                          description: >-
                            Degrees offset of the stadium orientation from true
                            north.
                          type: string
                          example: '30'
                        teams:
                          type: object
                          description: Map of team IF to that team’s season record.
                          additionalProperties:
                            type: object
                            properties:
                              record:
                                type: string
                                description: >-
                                  Season record, e.g. “12-5”; may be empty if
                                  not yet available.
                                example: ''
                        away_probable_id:
                          type: integer
                          format: integer
                          description: >-
                            player_id for the away team’s probable starting
                            pitcher.
                          example: 4655
                        home_probable_id:
                          type: integer
