openapi: 3.1.1
info:
  title: FantasyPros Public API
  description: >-
    This is the free limited public API. To request an API key contact us at
    api@fantasypros.com. The base url for this API is:
    https://api.fantasypros.com/public/v2/json
  contact:
    name: FantasyPros
    url: https://www.fantasypros.com
    email: api@fantasypros.com
  version: '2.0'
  termsOfService: https://api.fantasypros.com/public/v2/terms-of-use
  x-logo:
    url: >-
      https://images.fantasypros.com/images/branding/fantasypros-fullcolor-light-bg.svg
    altText: FantasyPros logo
servers:
  - url: https://api.fantasypros.com/public/v2/json
    description: test
paths:
  /{sport}/players:
    get:
      summary: Players
      parameters:
        - $ref: '#/components/parameters/Sport'
        - name: player
          description: A FantasyPros player ID to filter results on
          in: query
          required: false
          schema:
            type: integer
            default: null
            example: 6880
        - name: update
          description: >-
            Only return players that have been updated since this date. Format
            YYYY-MM-DD
          in: query
          required: false
          schema:
            type: string
            default: null
            example: '2025-05-13'
        - name: ecr
          description: >-
            A flag on whether to include or exclude players that are in
            consensus rankings.
          in: query
          required: false
          schema:
            type: string
            enum:
              - included
              - excluded
              - null
            default: null
            example: included
        - name: external_ids
          in: query
          description: >-
            Colon-delimited string of external IDs to include in the player
            object
          required: false
          schema:
            type: string
            pattern: ^(\w+)((?:\:\w+)+)?$
            default: null
            enum:
              - yahoo
              - espn
              - cbs
              - rts
              - fanduel
              - draftkings
              - fantasydraft
              - rotogrinders
              - fleaflicker
              - rotowire
              - rotoworld
              - numberfire
              - fantrax
              - nfl
              - mfl
              - tsn
              - onroto
              - xmlteam
              - ffwc
              - mlbam
              - nba
              - null
            example: yahoo:espn:cbs
        - name: show
          in: query
          description: Show the players positional consensus ranking in addition to overall
          required: false
          schema:
            type: string
            default: null
            enum:
              - pos_rank
              - null
            example: pos_rank
      responses:
        '200':
          description: A successful response
          content:
            application/json:
              schema:
                oneOf:
                  - $ref: '#/components/schemas/NFLPlayerResponse'
                  - $ref: '#/components/schemas/MLBPlayerResponse'
                  - $ref: '#/components/schemas/NBAPlayerResponse'
                  - $ref: '#/components/schemas/NHLPlayerResponse'
                discriminator:
                  propertyName: sport
                  mapping:
                    NFL: '#/components/schemas/NFLPlayerResponse'
                    MLB: '#/components/schemas/MLBPlayerResponse'
                    NBA: '#/components/schemas/NBAPlayerResponse'
                    NHL: '#/components/schemas/NHLPlayerResponse'
      security:
        - api_key: []
  /{sport}/news:
    get:
      summary: News
      description: Returns player news updates by sport
      parameters:
        - $ref: '#/components/parameters/Sport'
        - name: MLBAMID
          description: A MLBAM player ID. Only used for MLB
          in: query
          required: false
          schema:
            type: integer
            example: 701350
            default: null
        - name: fpid
          description: A FantasyPros player id to filter returned news
          in: query
          required: false
          schema:
            type: integer
            example: 6880
            default: null
        - name: limit
          description: The number of news items to be returned
          in: query
          required: false
          schema:
            type: integer
            maximum: 100
            default: 25
        - name: category
          description: The type of news items to return
          in: query
          required: false
          schema:
            type: string
            enum:
              - injury
              - recap
              - transaction
              - rumor
              - breaking
              - null
            default: null
        - name: order_by
          description: The order by direction for the news items
          in: query
          required: false
          schema:
            type: string
            enum:
              - updated
              - created
            default: created
      responses:
        '200':
          description: A successful response
          content:
            application/json:
              schema:
                type: object
                required:
                  - sport
                  - title
                  - description
                  - count
                  - items
                properties:
                  sport:
                    type: string
                    example: MLB
                  title:
                    type: string
                    example: Fantasy Player News
                  description:
                    type: string
                    example: >-
                      Breaking Fantasy Baseball player news along with the
                      impact for fantasy managers
                  count:
                    type: integer
                    example: 25
                  items:
                    type: array
                    items:
                      type: object
                      required:
                        - id
                        - created
                        - created_formated
                        - author
                        - player_id
                        - team_id
                        - title
                        - sport_id
                        - categories
                        - link
                        - desc
                        - impact
                      properties:
                        id:
                          type: integer
                          example: 51970
                        created:
                          type: string
                          example: '2025-05-12 07:29:02'
                        created_formated:
                          type: string
                          example: Mon, May 12th 7:29am UTC
                        author:
                          type: string
                          example: Ari Koslow
                        player_id:
                          $ref: '#/components/schemas/PlayerId'
                        team_id:
                          $ref: '#/components/schemas/TeamId'
                        title:
                          type: string
                          example: Caleb Freeman optioned to Triple-A
                        sport_id:
                          type: string
                          example: MLB
                        categories:
                          type: array
                          items:
                            type: string
                          example:
                            - Commentary
                            - News
                        link:
                          type: string
                          format: url
                          example: >-
                            https://www.fantasypros.com/mlb/news/519470/caleb-freeman-optioned-to-triple-a.php
                        desc:
                          type: string
                          example: >-
                            The White Sox optioned RHP Caleb Freeman to Triple-A
                            Charlotte.
                        impact:
                          type: string
                          example: >-
                            Freeman is sent back down one week after he had his
                            contract selected. The White Sox needed to open a
                            roster spot after claiming Yoendys Gómez off waivers
                            from the Dodgers.
      security:
        - api_key: []
  /{sport}/injuries:
    get:
      summary: Injuries
      description: Returns details about player injuries.
      parameters:
        - $ref: '#/components/parameters/Sport'
        - name: year
          in: query
          required: false
          schema:
            type: integer
            example: 2025
        - name: week
          description: NFL week to request injuries for. (NFL ONLY)
          in: query
          schema:
            type: integer
            minimum: 0
            example: 0
        - name: include_minors
          description: Include minor league players as injury statuses (MLB ONLY)
          in: query
          schema:
            type: string
            enum:
              - 'true'
            example: 'true'
        - name: include_probabilities
          description: >-
            Include players that might not have an injury status but are on the
            practice report. (NFL ONLY)
          in: query
          schema:
            type: string
            enum:
              - 'true'
            example: 'true'
        - name: team_id
          description: Colon delimited list of team_ids to filter response by
          in: query
          schema:
            $ref: '#/components/schemas/wordColon'
          examples:
            singleTeam:
              summary: A single team_id
              value: SF
            multipleTeams:
              summary: Multiple team_ids
              value: SF:MIN
        - name: player_ids
          description: Colon delimited list of FP player IDs to filter response by
          in: query
          schema:
            $ref: '#/components/schemas/digitColon'
          examples:
            singlePlayer:
              summary: A single player
              value: '7354'
            multiplePlayers:
              summary: Multiple players
              value: '7354:6880'
      responses:
        '200':
          description: A successful response
          content:
            application/json:
              schema:
                oneOf:
                  - $ref: '#/components/schemas/NFLInjuryResponse'
                  - $ref: '#/components/schemas/MLBInjuryResponse'
                  - $ref: '#/components/schemas/NBAInjuryResponse'
                  - $ref: '#/components/schemas/NHLInjuryResponse'
                discriminator:
                  propertyName: sport
                  mapping:
                    NFL: '#/components/schemas/NFLInjuryResponse'
                    MLB: '#/components/schemas/MLBInjuryResponse'
                    NBA: '#/components/schemas/NBAInjuryResponse'
                    NHL: '#/components/schemas/NHLInjuryResponse'
      security:
        - api_key: []
  /{sport}/compare-players:
    get:
      summary: Compare Players
      description: Compare 2-4 players based on rankings
      parameters:
        - $ref: '#/components/parameters/Sport'
        - name: year
          in: query
          required: false
          schema:
            type: integer
            example: 2025
        - name: week
          description: NFL week to request injuries for. (NFL ONLY)
          in: query
          required: false
          schema:
            type: integer
            minimum: 0
            example: 0
        - name: players
          description: Colon delimited list of FP player IDs to compare
          in: query
          required: true
          schema:
            $ref: '#/components/schemas/digitColon'
          examples:
            singlePlayer:
              summary: A single player
              value: '7354'
            multiplePlayers:
              summary: Multiple players
              value: '7354:6880'
        - $ref: '#/components/parameters/Position'
        - name: experts
          description: Colon delimited list of expert IDs to compare
          in: query
          required: false
          schema:
            $ref: '#/components/schemas/digitColon'
          examples:
            singlePlayer:
              summary: A single expert
              value: '232'
            multiplePlayers:
              summary: Multiple multiple experts
              value: '232:6633'
        - name: ranking_type
          description: The name of the ranking type you wish to compare rankings for
          required: false
          in: query
          schema:
            enum:
              - draft
              - weekly
              - ros
            type: string
            example: draft
        - name: details
          description: Include details about experts, player or both in the response
          required: false
          in: query
          schema:
            enum:
              - players
              - experts
              - all
            type: string
            example: all
      responses:
        '200':
          description: A successful response
          content:
            application/json:
              schema:
                oneOf:
                  - $ref: '#/components/schemas/NFLComparePlayersResponse'
                  - $ref: '#/components/schemas/MLBComparePlayersResponse'
                  - $ref: '#/components/schemas/NBAComparePlayersResponse'
                  - $ref: '#/components/schemas/NHLComparePlayersResponse'
                discriminator:
                  propertyName: sport
                  mapping:
                    NFL: '#/components/schemas/NFLComparePlayersResponse'
                    MLB: '#/components/schemas/MLBComparePlayersResponse'
                    NBA: '#/components/schemas/NBAComparePlayersResponse'
                    NHL: '#/components/schemas/NHLComparePlayersResponse'
      security:
        - api_key: []
  /{sport}/{season}/rankings:
    get:
      summary: Rankings
      description: Returns player rankings for multiple ranking types and positions.
      parameters:
        - $ref: '#/components/parameters/Season'
        - $ref: '#/components/parameters/Sport'
        - name: week
          in: query
          schema:
            type: integer
            minimum: 0
            example: 0
        - name: player
          description: A FantasyPros player ID to filter results on
          in: query
          required: false
          schema:
            type: integer
            default: null
            example: 6880
        - $ref: '#/components/parameters/Filters'
        - name: min
          description: >-
            Controls the level of player information returned in the response. A
            value of `true` will return less player information for a smaller
            response.
          in: query
          required: false
          schema:
            default: 'false'
            type: string
            enum:
              - 'true'
              - 'false'
            example: 'true'
        - name: range
          description: Display the min and max rank for a player
          in: query
          required: false
          schema:
            default: 'false'
            type: string
            enum:
              - 'true'
              - 'false'
            example: true
        - name: rankstats
          description: Display the average and standard deviation of a ranking
          in: query
          required: false
          schema:
            default: 'false'
            type: string
            enum:
              - 'true'
              - 'false'
            example: true
        - name: type
          description: Include Drafters ranking type (NFL ONLY)
          in: query
          required: false
          schema:
            type: string
            enum:
              - DRAFTERS
            example: DRAFTERS
        - $ref: '#/components/parameters/MLBEligibility'
      responses:
        '200':
          description: A successful response
          content:
            application/json:
              schema:
                oneOf:
                  - $ref: '#/components/schemas/NFLPlayerRankingResponse'
                  - $ref: '#/components/schemas/MLBPlayerRankingResponse'
                  - $ref: '#/components/schemas/NBAPlayerRankingResponse'
                  - $ref: '#/components/schemas/NHLPlayerRankingResponse'
                discriminator:
                  propertyName: sport
                  mapping:
                    NFL: '#/components/schemas/NFLPlayerRankingResponse'
                    MLB: '#/components/schemas/MLBPlayerRankingResponse'
                    NBA: '#/components/schemas/NBAPlayerRankingResponse'
                    NHL: '#/components/schemas/NHLPlayerRankingResponse'
      security:
        - api_key: []
  /{sport}/{season}/consensus-rankings:
    get:
      summary: Consensus Rankings
      description: Returns detailed consensus player rankings by ranking type and position.
      parameters:
        - $ref: '#/components/parameters/Season'
        - $ref: '#/components/parameters/Sport'
        - $ref: '#/components/parameters/Position'
        - $ref: '#/components/parameters/RankingType'
        - $ref: '#/components/parameters/Scoring'
        - name: week
          in: query
          schema:
            type: integer
            minimum: 0
            example: 0
        - name: include_idp
          in: query
          schema:
            type: string
            enum:
              - 'true'
            example: 'true'
        - $ref: '#/components/parameters/Filters'
        - name: experts
          description: Include details about the experts included in the ranking set
          in: query
          schema:
            type: string
            enum:
              - show
              - available
            example: show
      responses:
        '200':
          description: A successful response
          content:
            application/json:
              schema:
                oneOf:
                  - $ref: '#/components/schemas/NFLRankingsResponse'
                  - $ref: '#/components/schemas/MLBRankingsResponse'
                  - $ref: '#/components/schemas/NBARankingsResponse'
                  - $ref: '#/components/schemas/NHLRankingsResponse'
                discriminator:
                  propertyName: sport
                  mapping:
                    NFL: '#/components/schemas/NFLRankingsResponse'
                    MLB: '#/components/schemas/MLBRankingsResponse'
                    NBA: '#/components/schemas/NBARankingsResponse'
                    NHL: '#/components/schemas/NHLRankingsResponse'
        '400':
          description: 400 response
          content:
            application/json:
              schema:
                type: object
                properties:
                  message:
                    description: A message describing the error
                    type: string
                    example: Invalid Position
                  parameter:
                    description: Which parameter there is an error for
                    type: string
                    example: position
                  valid_format:
                    description: A valid value for the parameter
                    type: string
                    example: QB, RB, WR, TE, K, DST
      security:
        - api_key: []
  /{sport}/{season}/rankings/experts:
    get:
      summary: Experts
      description: Returns detailed information on experts.
      parameters:
        - $ref: '#/components/parameters/Season'
        - $ref: '#/components/parameters/Sport'
        - $ref: '#/components/parameters/PositionNotRequired'
        - $ref: '#/components/parameters/RankingType'
        - $ref: '#/components/parameters/Scoring'
        - name: include_overall
          description: Include overall ranking accuracy in response
          in: query
          schema:
            type: string
            enum:
              - 'true'
            example: 'true'
      responses:
        '200':
          description: 200 response
          content:
            application/json:
              schema:
                oneOf:
                  - $ref: '#/components/schemas/NFLExpertsResponse'
                  - $ref: '#/components/schemas/MLBExpertsResponse'
                  - $ref: '#/components/schemas/NBAExpertsResponse'
                  - $ref: '#/components/schemas/NHLExpertsResponse'
                discriminator:
                  propertyName: sport
                  mapping:
                    NFL: '#/components/schemas/NFLExpertsResponse'
                    MLB: '#/components/schemas/MLBExpertsResponse'
                    NBA: '#/components/schemas/NBAExpertsResponse'
                    NHL: '#/components/schemas/NHLExpertsResponse'
      security:
        - api_key: []
  /nfl/{season}/projections:
    get:
      summary: NFL Projections
      description: Returns player projections.
      parameters:
        - $ref: '#/components/parameters/Season'
        - $ref: '#/components/parameters/Position'
        - $ref: '#/components/parameters/Filters'
        - name: positions
          description: Colon delimited list of positions to filter response by
          in: query
          schema:
            $ref: '#/components/schemas/wordColon'
        - name: players
          description: Colon delimited list of FP player IDs to filter response by
          in: query
          schema:
            $ref: '#/components/schemas/digitColon'
          examples:
            singlePlayer:
              summary: A single player
              value: '7354'
            multiplePlayers:
              summary: Multiple players
              value: '7354:6880'
        - name: week
          description: >-
            The week to request projections for use week = 0 for preseason
            projections.
          in: query
          schema:
            type: integer
            example: 4
      responses:
        '200':
          description: 200 response
          content:
            application/json:
              schema:
                type: object
                required:
                  - season
                  - week
                  - count
                  - positions
                  - scoring
                  - players
                properties:
                  season:
                    type: string
                    example: 2025
                  week:
                    type: string
                    example: 0
                  count:
                    type: string
                    example: 194
                  positions:
                    type: string
                    example: RB
                  scoring:
                    type: string
                    example: STD
                  players:
                    type: array
                    items:
                      type: object
                      required:
                        - fpid
                        - mflid
                        - name
                        - position_id
                        - team_id
                        - filename
                        - stats
                      properties:
                        fpid:
                          type: string
                          example: 17240
                        mflid:
                          type: string
                          example: 13604
                        name:
                          type: string
                          example: Saquon Barkley
                        position_id:
                          type: string
                          example: RB
                        team_id:
                          type: string
                          example: PHI
                        filename:
                          type: string
                          example: saquon-barkley.php
                        stats:
                          type: array
                          items:
                            oneOf:
                              - $ref: '#/components/schemas/NFLQBPlayerProjections'
                              - $ref: >-
                                  #/components/schemas/NFLRBWRTEPlayerProjections
                              - $ref: '#/components/schemas/NFLDSTPlayerProjections'
                              - $ref: '#/components/schemas/NFLIDPPlayerProjections'
      security:
        - api_key: []
  /mlb/{season}/projections:
    get:
      summary: MLB Projections
      description: Returns MLB player projections.
      parameters:
        - $ref: '#/components/parameters/Season'
        - $ref: '#/components/parameters/Filters'
        - $ref: '#/components/parameters/MLBEligibility'
        - name: fpIds
          description: Comma delimited list of FP player IDs to filter response by
          in: query
          schema:
            $ref: '#/components/schemas/digitComma'
          examples:
            singlePlayer:
              summary: A single player
              value: '7354'
            multiplePlayers:
              summary: Multiple players
              value: 7354,6880
        - name: type
          description: Type of projections, rest of season, daily or weekly
          in: query
          schema:
            type: string
            enum:
              - ros
              - daily
              - weekly
              - preseason
            example: ros
            default: preseason
        - name: position
          in: query
          schema:
            $ref: '#/components/schemas/MLBPositions'
        - name: date
          description: Date to return for daily projections
          in: query
          schema:
            type: string
            example: '2025-05-13'
        - name: week
          description: Week number to return for weekly projections
          in: query
          schema:
            type: integer
            example: 4
        - name: league_key
          description: An MPB/DW league key
          in: query
          schema:
            $ref: '#/components/schemas/mpbLeagueKey'
      responses:
        '200':
          description: A successful response
          content:
            application/json:
              schema:
                type: object
                required:
                  - season
                  - week
                  - date
                  - type
                  - position
                  - experts
                  - position_name
                  - count
                  - player
                properties:
                  season:
                    type: string
                    example: 2024
                  week:
                    type: string
                    example: 0
                  date:
                    type: string
                    example: '2025-05-19'
                  type:
                    type: string
                    enum:
                      - ros
                      - daily
                      - weekly
                      - preseason
                    example: preseason
                  position:
                    $ref: '#/components/schemas/MLBPositions'
                  experts:
                    description: >-
                      An array containing the expert_ids included in the
                      projection data
                    type: string
                    example:
                      - 71
                      - 538
                      - 1370
                      - 1369
                      - 224
                      - 230
                      - 231
                      - 3916
                      - 650
                      - 7491
                  position_name:
                    description: The full name of the positon returned in the response
                    example: First Basemen
                  count:
                    type: integer
                    example: 291
                  player:
                    description: An array of player projection objects
                    type: array
                    items:
                      oneOf:
                        - $ref: '#/components/schemas/MLBHPlayerProjections'
                        - $ref: '#/components/schemas/MLBPPlayerProjections'
      security:
        - api_key: []
  /nba/{season}/projections:
    get:
      summary: NBA Projections
      description: Returns NBA player projections.
      parameters:
        - $ref: '#/components/parameters/Season'
        - $ref: '#/components/parameters/Filters'
        - name: fpIds
          description: Comma delimited list of FP player IDs to filter response by
          in: query
          schema:
            $ref: '#/components/schemas/digitComma'
          examples:
            singlePlayer:
              summary: A single player
              value: '7354'
            multiplePlayers:
              summary: Multiple players
              value: 7354,6880
        - name: team_id
          description: Comma delimited list of team IDs to filter response by
          in: query
          schema:
            $ref: '#/components/schemas/digitComma'
          examples:
            singlePlayer:
              summary: A single team
              value: MIL
            multiplePlayers:
              summary: Multiple teams
              value: MIL,BOS
        - name: stat_values
          description: Show more precise values for projected values
          in: query
          schema:
            type: string
            enum:
              - precise
        - name: stype
          description: Stats type, totals or averages
          in: query
          schema:
            type: string
            enum:
              - total
              - avg
            example:
              - total
            default: total
        - name: type
          description: Type of projections, rest of season, daily or weekly
          in: query
          schema:
            type: string
            enum:
              - ros
              - daily
              - weekly
              - preseason
            example: ros
            default: preseason
        - name: position
          in: query
          schema:
            $ref: '#/components/schemas/NBAPositions'
        - name: date
          description: Date to return for daily projections
          in: query
          schema:
            type: string
            example: '2025-05-13'
      responses:
        '200':
          description: A successful response
          content:
            application/json:
              schema:
                type: object
                required:
                  - season
                  - week
                  - date
                  - type
                  - position
                  - count
                  - player
                properties:
                  season:
                    type: string
                    example: 2024
                  week:
                    type: string
                    example: 0
                  date:
                    type: string
                    example: '2025-05-19'
                  type:
                    type: string
                    enum:
                      - ros
                      - daily
                      - weekly
                      - preseason
                    example: preseason
                  position:
                    $ref: '#/components/schemas/NBAPositions'
                  count:
                    type: integer
                    example: 291
                  player:
                    description: An array of player projection objects
                    type: array
                    items:
                      type: object
                      required:
                        - fpid
                        - player_id
                        - yahooid
                        - nbacom_player_id
                        - name
                        - position_id
                        - team_id
                        - player_page_url
                        - points
                        - rebounds
                        - assists
                        - blocks
                        - steals
                        - field_goals_pct
                        - free_throws_pct
                        - three_points_made
                        - games_played
                        - games_started
                        - minutes
                        - field_goals_made
                        - field_goals_att
                        - field_goals_missed
                        - three_points_att
                        - blocked_att
                        - three_points_missed
                        - free_throws_made
