    wordColon:
      description: A single word or string of colon delimited words
      type: string
      pattern: ^(\w+)((?:\:\w+)+)?$
    digitColon:
      description: A single ID or string of colon delimited IDs
      type: string
      pattern: ^(\d+)((?:\:\d+)+)?$
    digitComma:
      description: A single ID or string of comma delimited IDs
      type: string
      pattern: ^(\d+)((?:\,\d+)+)?$
    mpbLeagueKey:
      description: An MPB/DW league key
      type: string
      pattern: >-
        ^(mlb|nfl|nba)~[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$
      example: nfl~cd789a16-00f2-43c0-af80-f9d876c9e33e
  parameters:
    Sport:
      name: sport
      in: path
      required: true
      schema:
        $ref: '#/components/schemas/Sport'
    Season:
      name: season
      in: path
      required: true
      schema:
        $ref: '#/components/schemas/Season'
    Position:
      name: position
      in: query
      required: true
      schema:
        $ref: '#/components/schemas/SPORTPositions'
    PositionNotRequired:
      name: position
      in: query
      schema:
        $ref: '#/components/schemas/SPORTPositions'
    RankingType:
      name: type
      in: query
      schema:
        $ref: '#/components/schemas/SPORTRankingTypes'
    Scoring:
      name: scoring
      in: query
      schema:
        $ref: '#/components/schemas/SPORTScoringTypes'
    NFLPosition:
      name: position
      description: Only return players with this position.
      in: query
      required: false
      schema:
        $ref: '#/components/schemas/NFLPositions'
    Filters:
      name: filters
      description: A comma delimited string of expert IDs filter rankings by
      in: query
      schema:
        type: string
        pattern: ^(\d+)((?:\:\d+)+)?$
        example: '345:332:12'
    MLBEligibility:
      name: site_eligibility
      description: Use this fantasy site's position eligibility (MLB ONLY)
      in: query
      required: false
      schema:
        type:
          - string
          - integer
        enum:
          - AO
          - E
          - 'Y'
          - C
          - CBSSP
          - FANSP
          - 1
          - 2
          - 3
          - 103
          - 122
  securitySchemes:
    api_key:
      type: apiKey
      name: x-api-key
      in: header
