                          description: >-
                            player_id for the home team’s probable starting
                            pitcher.
                          example: 6995
                        hitters:
                          type: object
                          description: Batting order by team, keyed by lineup position.
                          additionalProperties:
                            type: object
                            description: Map of lineup slot to hitter info.
                            additionalProperties:
                              type: object
                              required:
                                - player_id
                                - rank_vbr
