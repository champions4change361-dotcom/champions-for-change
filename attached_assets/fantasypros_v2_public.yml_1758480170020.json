    ConsensusRankings:
      type: object
      required:
        - sport
        - year
        - week
        - filters
        - count
        - total_experts
        - last_updated
        - last_updated_ts
        - players
      properties:
        sport:
          $ref: '#/components/schemas/Sport'
        year:
          type: string
          example: '2023'
        week:
          type: string
          example: '0'
        filters:
          type:
            - string
            - null
          example: 6297,6318,375,4317,908,5446
        count:
          type: integer
          example: 40
        total_experts:
          type: integer
          example: 25
        last_updated:
          type: string
          pattern: \d+\/\d+
          example: 04/15
        last_updated_ts:
          type: integer
          example: 1747173394
        expert_pub:
          type: object
          description: Map of expert-ID (as string) → publication timestamp
          propertyNames:
            type: string
            pattern: ^[0-9]+$
          additionalProperties:
            type: string
            format: date-time
          example:
            '7': '2022-04-06 13:10:02'
            '9': '2022-04-07 15:59:20'
            '22': '2022-04-01 20:36:52'
        expert_name:
          type: object
          description: Map of expert-ID → expert’s full name
          propertyNames:
            type: string
            pattern: ^[0-9]+$
          additionalProperties:
            type: string
          example:
            '3': Eric Karabell
            '5': Staff Composite
            '6': Brandon Funston
        expert_twitter:
          type: object
          description: Map of expert-ID → Twitter handle
          propertyNames:
            type: string
            pattern: ^[0-9]+$
          additionalProperties:
            type: string
          example:
            '5': ESPN
            '6': BrandonFunston
            '7': andybehrens
        expert_image_url:
          type: object
          description: Map of expert-ID → profile image URL
          propertyNames:
            type: string
            pattern: ^[0-9]+$
          additionalProperties:
            type: string
            format: uri
          example:
            '3': >-
              https://pbs.twimg.com/profile_images/654082377694715904/4BOF1NJ4.jpg
            '5': >-
              https://pbs.twimg.com/profile_images/903763131872288768/lR7-Fb1N.jpg
            '6': >-
              https://pbs.twimg.com/profile_images/1545103401544925184/BHhf-ZbA.jpg
        experts_available:
          type: object
          description: >-
            Metadata about which experts are available and when it was last
            updated
          required:
            - total
            - included
            - excluded
            - last_update
          properties:
            total:
              type: integer
              description: Total number of experts
              example: 48
            included:
              type: array
              description: List of expert IDs to include
              items:
                type: integer
              example:
                - 9
                - 174
                - 2420
                - 759
                - 2475
                - 394
                - 406
                - 203
                - 766
                - 3040
            excluded:
              type: array
              description: List of expert IDs to exclude
              items:
                type: integer
              example: []
            last_update:
              type: integer
              description: Epoch-seconds timestamp of when the list was last refreshed
              example: 1649347160
        players:
          type: array
