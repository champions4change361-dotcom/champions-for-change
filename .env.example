# AI Training Configuration (Development Only)
# Set this to 'true' in development to enable hidden AI training features
# These features are completely disabled in production and will not affect deployments
ENABLE_AI_TRAINING=false

# Required for AI training (only needed if ENABLE_AI_TRAINING=true)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Production Database
DATABASE_URL=your_production_database_url

# Required Stripe Keys
STRIPE_SECRET_KEY=your_stripe_secret_key
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key

# Email Service
SENDGRID_API_KEY=your_sendgrid_api_key

# Session Security
SESSION_SECRET=your_session_secret_key

# OAuth Domains
REPLIT_DOMAINS=your_domain_here.replit.dev